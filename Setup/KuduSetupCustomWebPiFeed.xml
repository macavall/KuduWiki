  <entry>
    <productId>VS2013_CE_Only_Wrapper</productId>
    <metadata>
      <title>Kudu Visual Studio Community Edition 2013 Feed Hook</title>
      <summary>Kudu Visual Studio Community Edition 2013 Feed Hook</summary>
      <version>12.0.0.0</version>
      <images>
        <icon>http://go.microsoft.com/fwlink/?LinkId=316858</icon>
      </images>
      <author>
        <name>Microsoft</name>
        <uri>http://www.microsoft.com/</uri>
      </author>
      <summaryExclude />
    </metadata>
    <dependency>
      <productId>VS2013_CE_Only</productId>
    </dependency>
    <!-- 
			Visual Studio 2013: HKEY_CLASSES_ROOT\VisualStudio.DTE.12.0
			Visual Studio 2014: HKEY_CLASSES_ROOT\VisualStudio.DTE.13.0
			Visual Studio 2015: HKEY_CLASSES_ROOT\VisualStudio.DTE.14.0
			Visual Studio Express 2013: HKEY_CLASSES_ROOT\VWDExpress.DTE.12.0
			which mean, if machine already if these VS installed, we will skip VS installation
		-->
    <discoveryHint>
      <or>
        <discoveryHint>
          <registry>
            <keyPath>HKEY_CLASSES_ROOT\VisualStudio.DTE.12.0</keyPath>
          </registry>
        </discoveryHint>
		<discoveryHint>
          <registry>
            <keyPath>HKEY_CLASSES_ROOT\VisualStudio.DTE.13.0</keyPath>
          </registry>
        </discoveryHint>
		<discoveryHint>
          <registry>
            <keyPath>HKEY_CLASSES_ROOT\VisualStudio.DTE.14.0</keyPath>
          </registry>
        </discoveryHint>
        <discoveryHint>
          <registry>
            <keyPath>HKEY_CLASSES_ROOT\VWDExpress.DTE.12.0</keyPath>
          </registry>
        </discoveryHint>
      </or>
    </discoveryHint>
    <installers>
      <installer>
        <id>1</id>
        <languageId>en</languageId>
        <installCommands>
          <cmdline>
            <exe>%windir%\system32\cmd.exe</exe>
            <commandLineArguments>/C echo noop</commandLineArguments>
          </cmdline>
        </installCommands>
      </installer>
    </installers>
  </entry>

