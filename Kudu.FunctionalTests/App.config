<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appSettings>
        <!-- Uncomment out this line to reuse the same site for all functional tests, which runs much faster -->
        <add key="SiteReusedForAllTests" value="TestRunnerSite"/>
        <!-- Set to false to test original site builder factory -->
        <add key="TestGeneratorSiteBuilderFactory" value="true"/>
    </appSettings>
    <system.net>
        <connectionManagement>
          <clear />
          <add address="*" maxconnection="10" />
        </connectionManagement>
    </system.net>
</configuration>